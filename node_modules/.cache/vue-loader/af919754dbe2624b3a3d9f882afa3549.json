{"remainingRequest":"D:\\Xampp\\htdocs\\FrontEnd\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\Xampp\\htdocs\\FrontEnd\\src\\views\\construccion\\projects\\Detalle.vue?vue&type=style&index=0&id=3477ad85&scoped=true&lang=css&","dependencies":[{"path":"D:\\Xampp\\htdocs\\FrontEnd\\src\\views\\construccion\\projects\\Detalle.vue","mtime":1637077085868},{"path":"D:\\Xampp\\htdocs\\FrontEnd\\node_modules\\css-loader\\dist\\cjs.js","mtime":1636401895556},{"path":"D:\\Xampp\\htdocs\\FrontEnd\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1636401901227},{"path":"D:\\Xampp\\htdocs\\FrontEnd\\node_modules\\postcss-loader\\src\\index.js","mtime":1636401897499},{"path":"D:\\Xampp\\htdocs\\FrontEnd\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1636401893889},{"path":"D:\\Xampp\\htdocs\\FrontEnd\\node_modules\\vue-loader\\lib\\index.js","mtime":1636401899309}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Detalle.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0ZA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA","file":"Detalle.vue","sourceRoot":"src/views/construccion/projects","sourcesContent":["<template>\r\n  <div>\r\n\r\n    <Navbar />\r\n    <NavbarSideBar />\r\n\r\n    <div class=\"content-wrapper\">\r\n      <!-- Content Header (Page header) -->\r\n      <div class=\"content-header\">\r\n        <div class=\"container-fluid\">\r\n          <div class=\"row mb-2\">\r\n            <div class=\"col-sm-6\">\r\n              <h1 class=\"m-0\">\r\n                {{ projectsName }}\r\n                <small v-if=\"! edit\">\r\n                  |\r\n                  <a href=\"javascript:;\" id=\"btnEditar\" @click=\"edit=true\">\r\n                                        <span class=\"right badge badge-secondary\">\r\n                                            Editar\r\n                                        </span>\r\n                  </a>\r\n                </small>\r\n              </h1>\r\n            </div><!-- /.col -->\r\n            <div class=\"col-sm-6\">\r\n              <ol class=\"breadcrumb float-sm-right\">\r\n                <li class=\"breadcrumb-item\"><router-link to=\"/Dashboard\">Dashboard</router-link></li>\r\n                <li class=\"breadcrumb-item\"><router-link to=\"/projects\">Projects</router-link></li>\r\n                <li class=\"breadcrumb-item active\">Detalle de registro # {{ getUrlQueryId() }}</li>\r\n              </ol>\r\n            </div><!-- /.col -->\r\n          </div><!-- /.row -->\r\n        </div><!-- /.container-fluid -->\r\n      </div>\r\n      <!-- /.content-header -->\r\n\r\n      <!-- Main content -->\r\n      <div class=\"content\" id=\"readonly\" v-if=\"!edit\">\r\n        <div class=\"container-fluid\">\r\n          <div class=\"card\">\r\n            <div class=\"container\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-12\">\r\n                  <table class=\"table table-striped table-hover\">\r\n\r\n                    <tbody>\r\n\r\n                    <tr>\r\n                      <th scope=\"row\">Short Projects Name:</th>\r\n                      <td>\r\n                        {{ newData.shortProjectsName }}\r\n                      </td>\r\n                    </tr>\r\n\r\n                    <tr>\r\n                      <th scope=\"row\">Status:</th>\r\n                      <td ><span :class=\"cssStatus\">{{ dataStatus }}</span></td>\r\n                    </tr>\r\n\r\n                    <tr>\r\n                      <th scope=\"row\">Division:</th>\r\n                      <td >{{ divisionName }}</td>\r\n                    </tr>\r\n\r\n                    </tbody>\r\n                  </table>\r\n\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"content\" id=\"edit\" v-else>\r\n        <div class=\"container-fluid\">\r\n          <div class=\"card\">\r\n            <div class=\"container\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-12\">\r\n\r\n                  <form>\r\n                    <table class=\"table table-striped table-hover\">\r\n\r\n                      <tbody>\r\n\r\n                      <tr>\r\n                        <th scope=\"row\">Short Projects Name:</th>\r\n                        <td v-if=\"edit\">\r\n                          <input\r\n                              type=\"text\"\r\n                              class=\"form-control\"\r\n                              placeholder=\"Short Projects Name\"\r\n                              v-model=\"newData.shortProjectsName\"\r\n                          />\r\n                        </td>\r\n                      </tr>\r\n                      <tr>\r\n                        <th scope=\"row\">Status:</th>\r\n                        <td >\r\n                          <input\r\n                              id=\"st\"\r\n                              type=\"checkbox\"\r\n                              class=\"\"\r\n                              :checked=\"chStatus\"\r\n                              @click=\"changeStatus()\"\r\n                          >\r\n                          <label for=\"st\" style=\"margin-left: 1em ;\" :class=\"cssStatus\">{{ dataStatus }}</label>\r\n                        </td>\r\n                      </tr>\r\n\r\n                      <tr>\r\n                        <th scope=\"row\">Division Assign to:</th>\r\n                        <td>\r\n                          <select\r\n                              name=\"\"\r\n                              id=\"\"\r\n                              class=\"form-control\"\r\n                              v-model=\"newData.divisionId\"\r\n                          >\r\n                            <option\r\n                                v-for = \"corp in divisiones.data\"\r\n                                :value = \"corp[ 0 ]\"\r\n                            >{{ corp[ 1 ] }}</option>\r\n                          </select>\r\n                        </td>\r\n                      </tr>\r\n\r\n                      </tbody>\r\n                    </table>\r\n                  </form>\r\n\r\n                  <div class=\"right\">\r\n                    <button class=\"btn btn-success\" id=\"btnEditar\" @click=\"saveChanges\"> Guardar cambios </button>\r\n                    <button class=\"btn btn-danger\" id=\"btnCancelar\" @click=\"cancelChanges\"> Cancelar edici√≥n </button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"content\" id=\"itZone\" v-if=\"IT\">\r\n        <div class=\"container-fluid\">\r\n          <div class=\"card\">\r\n            <div class=\"container\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-12\">\r\n                  <div>\r\n                    <h2>IT Zone</h2>\r\n                    <table class=\"table table-striped table-hover\">\r\n                      <tbody>\r\n\r\n                      <tr>\r\n                        <th scope=\"row\">Projects Name:</th>\r\n                        <td v-if=\"edit\">\r\n                          <input\r\n                              type=\"text\"\r\n                              class=\"form-control\"\r\n                              placeholder=\"Projects Name\"\r\n                              v-model=\"newData.projectsName\"\r\n                          />\r\n                        </td>\r\n                        <td v-else>\r\n                          {{ projectsName }}\r\n                        </td>\r\n                      </tr>\r\n\r\n\r\n\r\n                      <tr>\r\n                        <th>Date Last Modified</th>\r\n                        <td>{{ dateLastModified }}</td>\r\n                      </tr>\r\n\r\n                      <tr>\r\n                        <th>Last User Who Modified</th>\r\n                        <td>{{ createdBy }}</td>\r\n                      </tr>\r\n                      </tbody>\r\n\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from \"@/components/navbar/Navbar\";\r\nimport NavbarSideBar from \"@/components/navbar/NavbarSidebar\";\r\nimport requests from \"@/utils/requests\";\r\nimport errorCodes from \"@/utils/errorCodes\";\r\n\r\nexport default {\r\n  name: \"Details\"\r\n\r\n  , components: {\r\n    Navbar\r\n    , NavbarSideBar\r\n  }\r\n\r\n  , data() {\r\n    return {\r\n      edit : false\r\n      , IT : false\r\n      , id : null\r\n      , projectsName : null\r\n      , shortProjectsName : null\r\n      , divisiones : null\r\n      , divisionId : null\r\n      , divisionName : null\r\n      , dataStatus : null\r\n      , createdBy : null\r\n      , dateCreated : null\r\n      , chStatus : false\r\n      , ejercicioCerrado : null\r\n\r\n      , newData: {\r\n        projectsName : null\r\n        , shortProjectsName : null\r\n        , chStatus : null\r\n        , divisionId : null\r\n        , ejercicioCerrado : null\r\n      }\r\n\r\n      , cssStatus : \"stActive\"\r\n\r\n    }\r\n  }\r\n\r\n  , methods: {\r\n    getUrlQueryId() {\r\n      return this.$route.params.id ;\r\n    }\r\n\r\n    , async getRecordData() {\r\n      // let notification ;\r\n\r\n      const headers = {\r\n        headers: {\r\n          'Authorization': `Bearer ${ localStorage.getItem( 'accessToken' ) }`,\r\n          'WWW-Authenticate': `Basic fm483fj98f3lfir793dfu98itoqkp5t8.fm483fj98f3lfir793dfu98itoqkp5t8.fm483fj98f3lfir793dfu98itoqkp5t8/fm483fj98f3lfir793dfu98itoqkp5t8`,\r\n          'Accept': 'application/json',\r\n          'Content-type': 'application/json'\r\n        }\r\n      } ;\r\n\r\n      const response = await requests.get(\r\n          'projects/get/'+ this.getUrlQueryId()\r\n          , headers\r\n      ) ;\r\n\r\n      // console.log( \"response: \" , response.data ) ;\r\n\r\n      this.projectsName = response.data[ 1 ] ;\r\n      this.shortProjectsName = response.data[ 2 ] ;\r\n      this.divisionId = response.data [ 3 ][ 0 ][ 0 ] ;\r\n      this.divisionName = response.data [ 3 ][ 0 ][ 1 ] ;\r\n      this.chStatus = response.data [4][0].id === 1 ;\r\n      this.dataStatus = response.data [ 4 ] !== null ? response.data [ 4 ][ 0 ].stName  : \"\" ;\r\n      this.dateCreated = response.data [ 5 ] ;\r\n      this.createdBy = response.data [ 6 ] !== null ? response.data [ 6 ] : \"\" ;\r\n\r\n\r\n\r\n      this.newData.projectsName = this.projectsName ;\r\n      this.newData.shortProjectsName = this.shortProjectsName ;\r\n      this.newData.divisionId = this.divisionId ;\r\n      this.newData.chStatus = this.chStatus ;\r\n\r\n\r\n      this.cssStatus = this.chStatus === 1 || this.chStatus === true ? 'stActive' : 'stDeactivated' ;\r\n      this.edit = false ;\r\n\r\n      return response.data ;\r\n    }\r\n\r\n    , changeStatus() {\r\n      if ( this.newData.chStatus === true ) {\r\n        this.newData.chStatus = false ;\r\n        this.dataStatus = \"Deactivated\" ;\r\n        this.cssStatus = 'stDeactivated' ;\r\n      } else {\r\n        this.newData.chStatus = true ;\r\n        this.dataStatus = \"Active\" ;\r\n        this.cssStatus = 'stActive' ;\r\n      }\r\n    }\r\n\r\n    , async saveChanges() {\r\n\r\n      let notification ;\r\n\r\n      if ( this.newData.shortProjectsName.trim() === '' | this.newData.projectsName.trim() === '' ) {\r\n        notification = errorCodes.get( 400 ) ;\r\n\r\n        this.$notify({\r\n          group: 'foo'\r\n          , title: `<h4>Error Code: ${ notification.code }</h4>`\r\n          , text: `<h5>${ notification.message }</h5>\r\n                            <br />\r\n                            <h6>Fields should not left blank.</h6>\r\n                        `\r\n          , type: `${ notification.type }`\r\n          , duration: 5000\r\n        } ) ;\r\n      } else {\r\n\r\n        // console.log( this.newData.shortprojectsName ) ;\r\n\r\n        const headers = {\r\n          headers: {\r\n            'Authorization': `Bearer ${ localStorage.getItem( 'accessToken' ) }`,\r\n            'WWW-Authenticate': `Basic fm483fj98f3lfir793dfu98itoqkp5t8.fm483fj98f3lfir793dfu98itoqkp5t8.fm483fj98f3lfir793dfu98itoqkp5t8/fm483fj98f3lfir793dfu98itoqkp5t8`,\r\n            'Accept': 'application/json',\r\n            'Content-type': 'application/json'\r\n          }\r\n        }\r\n\r\n        let st = this.newData.chStatus === true ? 1 : 2 ;\r\n\r\n        const data = {\r\n          \"id\" : this.getUrlQueryId()\r\n          , \"projectsName\" : this.newData.projectsName\r\n          , \"projectsShortName\" : this.newData.shortProjectsName\r\n          , \"dataStatus\" : st\r\n          , \"divisionId\" : this.newData.divisionId\r\n        }\r\n\r\n        // console.log( \"data: \" , data ) ;\r\n\r\n        const response = await requests.put(\r\n            'projects/update'\r\n            , data\r\n            , headers\r\n        ) ;\r\n\r\n        // console.log( \"response: \" , response ) ;\r\n\r\n        if ( response.status === 200 ) {\r\n          await this.getRecordData() ;\r\n\r\n          notification = errorCodes.get( response.status ) ;\r\n          // console.log(notification)\r\n          this.$notify({\r\n            group: 'foo'\r\n            , title: `<h5>Message Code: ${notification.code}</h5>`\r\n            , text: `<h6>${notification.message}</h6>`\r\n            , type: `${notification.type}`\r\n            , duration: 10000\r\n          } ) ;\r\n          if ( st === 401 ) await this.$router.push( '/logout' ) ;\r\n\r\n        } else {\r\n          let st ;\r\n          if ( response.data !== typeof number ) st = 401 ;\r\n          else st = response.status | response.data ;\r\n\r\n          notification = errorCodes.get( st ) ;\r\n          // console.log(notification)\r\n          this.$notify({\r\n            group: 'foo'\r\n            , title: `<h5>Error Code: ${notification.code}</h5>`\r\n            , text: `<h6>${notification.message}</h6>`\r\n            , type: `${notification.type}`\r\n            , duration: -1\r\n          } ) ;\r\n          if ( st === 401 ) await this.$router.push( '/logout' ) ;\r\n        }\r\n      }\r\n\r\n    }\r\n\r\n    , cancelChanges() {\r\n      this.edit = false ;\r\n      this.getRecordData() ;\r\n    }\r\n\r\n    , async getDivisiones() {\r\n      const divisiones = await requests.get(\r\n          'divisiones/all'\r\n      ) ;\r\n\r\n      this.divisiones = divisiones.data ;\r\n    }\r\n\r\n  }\r\n\r\n  , created() {\r\n    this.getRecordData() ;\r\n    this.getDivisiones() ;\r\n  }\r\n\r\n  , mounted() {\r\n    if ( localStorage.getItem( \"roles\" )  === \"ROLE_IT\" ) this.IT = true ;\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.red {\r\n  background-color: red !important ;\r\n}\r\n\r\na#btnEditar:link,\r\na#btnEditar:hover,\r\na#btnEditar:visited {\r\n  font-weight: normal ;\r\n  color: lightblue ;\r\n  padding: .5em ;\r\n}\r\n\r\n#itZone {\r\n  margin-top: 2.5em ;\r\n}\r\n\r\n.right {\r\n  text-align: right ;\r\n}\r\n\r\n.btn-success {\r\n  margin-right: 2em ;\r\n}\r\n\r\n</style>"]}]}