EA,YAAA,KAAA,EAAA,qBAAA,YAAA,CAAA,IAAA,OAFA;AAGA,YAAA,IAAA,EAAA,OAAA,YAAA,CAAA,OAAA,OAHA;AAIA,YAAA,IAAA,EAAA,GAAA,YAAA,CAAA,IAAA,EAJA;AAKA,YAAA,QAAA,EAAA;AALA,WAAA;AAOA,cAAA,EAAA,KAAA,GAAA,EAAA,MAAA,KAAA,OAAA,CAAA,IAAA,CAAA,SAAA,CAAA;AAEA,SAdA,MAcA;AACA,cAAA,EAAA,CADA,CAEA;AACA;;AACA,UAAA,EAAA,GAAA,QAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AAEA,UAAA,YAAA,GAAA,UAAA,CAAA,GAAA,CAAA,EAAA,CAAA,CANA,CAOA;;AACA,eAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,KADA;AAEA,YAAA,KAAA,EAAA,mBAAA,YAAA,CAAA,IAAA,OAFA;AAGA,YAAA,IAAA,EAAA,OAAA,YAAA,CAAA,OAAA,OAHA;AAIA,YAAA,IAAA,EAAA,GAAA,YAAA,CAAA,IAAA,EAJA;AAKA,YAAA,QAAA,EAAA,CAAA;AALA,WAAA,EARA,CAeA;AACA;AACA;AAEA,KAhHA;;AAkHA,IAAA,aAAA,GAAA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,aAAA;AACA,KArHA;;AAuHA,UAAA,QAAA,CAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,KAAA,GAAA,KAAA,SAAA,IAAA,KAAA,GAAA,KAAA,IAAA,GAAA,CAAA,CAAA,EAAA,CAAA,CAAA,GAAA,GAAA;AAEA,WAAA,KAAA,GAAA,MAAA,QAAA,CAAA,IAAA,CACA,iBADA,EAEA,KAAA,GAFA,CAAA;AAIA;;AA9HA,GA5BA;;AA8JA,EAAA,OAAA,GAAA;AACA,SAAA,aAAA;AACA,SAAA,QAAA;AACA,GAjKA;;AAmKA,EAAA,OAAA,GAAA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,OAAA,MAAA,SAAA,EAAA,KAAA,EAAA,GAAA,IAAA;AACA;;AArKA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n\r\n        <Navbar />\r\n        <NavbarSideBar />\r\n\r\n        <div class=\"content-wrapper\">\r\n            <!-- Content Header (Page header) -->\r\n            <div class=\"content-header\">\r\n                <div class=\"container-fluid\">\r\n                    <div class=\"row mb-2\">\r\n                        <div class=\"col-sm-6\">\r\n                            <h1 class=\"m-0\">\r\n                                {{ moduleName }}\r\n                                <small v-if=\"! edit\">\r\n                                    |\r\n                                    <a href=\"javascript:;\" id=\"btnEditar\" @click=\"edit=true\">\r\n                                        <span class=\"right badge badge-secondary\">\r\n                                            Editar\r\n                                        </span>\r\n                                    </a>\r\n                                </small>\r\n                            </h1>\r\n                        </div><!-- /.col -->\r\n                        <div class=\"col-sm-6\">\r\n                            <ol class=\"breadcrumb float-sm-right\">\r\n                                <li class=\"breadcrumb-item\"><router-link to=\"/Dashboard\">Dashboard</router-link></li>\r\n                                <li class=\"breadcrumb-item\"><router-link to=\"/Modulos\">Modulos</router-link></li>\r\n                                <li class=\"breadcrumb-item active\">Detalle de registro # {{ getUrlQueryId() }}</li>\r\n                            </ol>\r\n                        </div><!-- /.col -->\r\n                    </div><!-- /.row -->\r\n                </div><!-- /.container-fluid -->\r\n            </div>\r\n            <!-- /.content-header -->\r\n\r\n            <!-- Main content -->\r\n            <div class=\"content\" id=\"readonly\" v-if=\"!edit\">\r\n                <div class=\"container-fluid\">\r\n                    <div class=\"card\">\r\n                        <div class=\"container\">\r\n                            <div class=\"row\">\r\n                                <div class=\"col-md-12\">\r\n                                    <table class=\"table table-striped table-hover\">\r\n\r\n                                        <tbody>\r\n                                            <tr>\r\n                                                <th scope=\"row\">Module Name:</th>\r\n                                                <td>\r\n                                                    {{ moduleName }}\r\n                                                </td>\r\n                                            </tr>\r\n\r\n                                            <tr>\r\n                                                <th scope=\"row\">Status:</th>\r\n                                                <td ><span :class=\"cssStatus\">{{ dataStatus }}</span></td>\r\n                                            </tr>\r\n\r\n                                        </tbody>\r\n                                    </table>\r\n\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"content\" id=\"edit\" v-else>\r\n                <div class=\"container-fluid\">\r\n                    <div class=\"card\">\r\n                        <div class=\"container\">\r\n                            <div class=\"row\">\r\n                                <div class=\"col-md-12\">\r\n\r\n                                    <form>\r\n                                        <table class=\"table table-striped table-hover\">\r\n\r\n                                            <tbody>\r\n\r\n                                                <tr>\r\n                                                    <th scope=\"row\">Module Name:</th>\r\n                                                    <td v-if=\"edit\">\r\n                                                        <input\r\n                                                            type=\"text\"\r\n                                                            class=\"form-control\"\r\n                                                            placeholder=\"Short Corp Name\"\r\n                                                            v-model=\"newData.moduleName\"\r\n                                                        />\r\n                                                    </td>\r\n                                                </tr>\r\n                                                <tr>\r\n                                                    <th scope=\"row\">Status:</th>\r\n                                                    <td >\r\n                                                        <input\r\n                                                            id=\"st\"\r\n                                                            type=\"checkbox\"\r\n                                                            class=\"\"\r\n                                                            :checked=\"chStatus\"\r\n                                                            @click=\"changeStatus()\"\r\n                                                        >\r\n                                                        <label for=\"st\" style=\"margin-left: 1em ;\" :class=\"cssStatus\">{{ dataStatus }}</label>\r\n                                                    </td>\r\n                                                </tr>\r\n\r\n                                            </tbody>\r\n                                        </table>\r\n                                    </form>\r\n\r\n                                    <div class=\"right\">\r\n                                        <button class=\"btn btn-success\" id=\"btnEditar\" @click=\"saveChanges\"> Guardar cambios </button>\r\n                                        <button class=\"btn btn-danger\" id=\"btnCancelar\" @click=\"cancelChanges\"> Cancelar edición </button>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"content\" id=\"itZone\" v-if=\"IT\">\r\n                <div class=\"container-fluid\">\r\n                    <div class=\"card\">\r\n                        <div class=\"container\">\r\n                            <div class=\"row\">\r\n                                <div class=\"col-md-12\">\r\n                                    <div>\r\n                                        <h2>IT Zone</h2>\r\n                                        <table class=\"table table-striped table-hover\">\r\n                                            <tbody>\r\n                                                <tr>\r\n                                                    <th>Date Last Modified</th>\r\n                                                    <td>{{ dateLastModified }}</td>\r\n                                                </tr>\r\n\r\n                                                <tr>\r\n                                                    <th>Last User Who Modified</th>\r\n                                                    <td>{{ userWhoModified }}</td>\r\n                                                </tr>\r\n                                            </tbody>\r\n\r\n                                        </table>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n        </div>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from \"@/components/navbar/Navbar\";\r\nimport NavbarSideBar from \"@/components/navbar/NavbarSidebar\";\r\nimport requests from \"@/utils/requests\";\r\nimport errorCodes from \"@/utils/errorCodes\";\r\n\r\nexport default {\r\n    name: \"Details\"\r\n\r\n    , components: {\r\n        Navbar\r\n        , NavbarSideBar\r\n    }\r\n\r\n    , data() {\r\n        return {\r\n            edit : false\r\n            , IT : false\r\n            , id : null\r\n            , moduleName : null\r\n            , dataStatus : null\r\n            , userWhoModified : null\r\n            , dateLastModified : null\r\n            , chStatus : false\r\n            , newData: {\r\n                moduleName : null\r\n                , chStatus : null\r\n            }\r\n\r\n            , cssStatus : \"stActive\"\r\n\r\n        }\r\n    }\r\n\r\n    , methods: {\r\n        getUrlQueryId() {\r\n            return this.$route.params.id ;\r\n        }\r\n\r\n        , async getRecordData() {\r\n            // let notification ;\r\n\r\n            const response = await requests.get(\r\n                'modulos/get/'+ this.getUrlQueryId()\r\n            ) ;\r\n\r\n            // console.log( \"response: \" , response.data ) ;\r\n\r\n            this.moduleName = response.data[ 1 ] ;\r\n            this.chStatus = response.data [2][0].id === 1 ;\r\n            this.dataStatus = response.data [ 2 ] !== null ? response.data [ 2 ][ 0 ].stName  : \"\" ;\r\n            this.userWhoModified = response.data [ 4 ] !== null ? response.data [ 4 ][ 0 ].username : \"\" ;\r\n            this.dateLastModified = response.data [ 5 ] ;\r\n\r\n            this.newData.moduleName = this.moduleName ;\r\n            this.newData.chStatus = this.chStatus ;\r\n\r\n            this.cssStatus = this.chStatus === 1 || this.chStatus === true ? 'stActive' : 'stDeactivated' ;\r\n            this.edit = false ;\r\n\r\n            return response.data ;\r\n        }\r\n\r\n        , changeStatus() {\r\n            if ( this.newData.chStatus === true ) {\r\n                this.newData.chStatus = false ;\r\n                this.dataStatus = \"Deactivated\" ;\r\n                this.cssStatus = 'stDeactivated' ;\r\n            } else {\r\n                this.newData.chStatus = true ;\r\n                this.dataStatus = \"Active\" ;\r\n                this.cssStatus = 'stActive' ;\r\n            }\r\n        }\r\n\r\n        , async saveChanges() {\r\n\r\n            let notification ;\r\n\r\n            if ( this.newData.moduleName.trim() === '' ) {\r\n                notification = errorCodes.get( 400 ) ;\r\n\r\n                this.$notify({\r\n                    group: 'foo'\r\n                    , title: `<h4>Error Code: ${ notification.code }</h4>`\r\n                    , text: `<h5>${ notification.message }</h5>\r\n                            <br />\r\n                            <h6>Fields should not left blank.</h6>\r\n                        `\r\n                    , type: `${ notification.type }`\r\n                    , duration: 5000\r\n                } ) ;\r\n            } else {\r\n\r\n                // console.log( this.newData.shortDivisionName ) ;\r\n\r\n                let st = this.newData.chStatus === true ? 1 : 2 ;\r\n\r\n                const data = {\r\n                    \"id\" : this.getUrlQueryId()\r\n                    , \"moduleName\" : this.newData.moduleName\r\n                    , \"dataStatus\" : st\r\n                }\r\n\r\n                // console.log( \"data: \" , data ) ;\r\n\r\n                const response = await requests.put(\r\n                    'modulos/update'\r\n                    , data\r\n                ) ;\r\n\r\n                // console.log( \"response: \" , response ) ;\r\n\r\n                if ( response.status === 200 ) {\r\n                    await this.getRecordData() ;\r\n\r\n                    notification = errorCodes.get( response.status ) ;\r\n                    // console.log(notification)\r\n                    this.$notify({\r\n                        group: 'foo'\r\n                        , title: `<h5>Message Code: ${notification.code}</h5>`\r\n                        , text: `<h6>${notification.message}</h6>`\r\n                        , type: `${notification.type}`\r\n                        , duration: 10000\r\n                    } ) ;\r\n                    if ( st === 401 ) await this.$router.push( '/logout' ) ;\r\n\r\n                } else {\r\n                    let st ;\r\n                    // if ( response.data !== typeof number ) st = 401 ;\r\n                    // else st = response.status | response.data ;\r\n                    st = response.status | response.data ;\r\n\r\n                    notification = errorCodes.get( st ) ;\r\n                    // console.log(notification)\r\n                    this.$notify({\r\n                        group: 'foo'\r\n                        , title: `<h5>Error Code: ${notification.code}</h5>`\r\n                        , text: `<h6>${notification.message}</h6>`\r\n                        , type: `${notification.type}`\r\n                        , duration: -1\r\n                    } ) ;\r\n                    // if ( st === 401 ) await this.$router.push( '/logout' ) ;\r\n                }\r\n            }\r\n\r\n        }\r\n\r\n        , cancelChanges() {\r\n            this.edit = false ;\r\n            this.getRecordData() ;\r\n        }\r\n\r\n        , async getCorps( arr ) {\r\n            this.arr = this.arr === undefined || this.arr === null ? [ 1,2 ] : arr ;\r\n\r\n            this.corps = await requests.post(\r\n                'corp/select-tag'\r\n                , this.arr\r\n            ) ;\r\n        }\r\n\r\n    }\r\n\r\n    , created() {\r\n        this.getRecordData() ;\r\n        this.getCorps() ;\r\n    }\r\n\r\n    , mounted() {\r\n        if ( localStorage.getItem( \"roles\" )  === \"ROLE_IT\" ) this.IT = true ;\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    .red {\r\n        background-color: red !important ;\r\n    }\r\n\r\n    a#btnEditarCorporativo:link,\r\n    a#btnEditarCorporativo:hover,\r\n    a#btnEditarCorporativo:visited {\r\n        font-weight: normal ;\r\n        color: lightblue ;\r\n        padding: .5em ;\r\n    }\r\n\r\n    #itZone {\r\n        margin-top: 2.5em ;\r\n    }\r\n\r\n    .right {\r\n        text-align: right ;\r\n    }\r\n\r\n    .btn-success {\r\n        margin-right: 2em ;\r\n    }\r\n\r\n</style>"],"sourceRoot":"src/views/administracion/modulos"}]}